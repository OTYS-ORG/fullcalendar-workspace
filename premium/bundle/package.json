{
  "name": "fullcalendar-scheduler",
  "version": "6.0.0-beta.1",
  "title": "FullCalendar Premium Bundle",
  "description": "An easily consumable combination of standard and premium plugins",
  "docs": "https://fullcalendar.io/docs/initialize-globals",
  "type": "module",
  "dependencies": {
    "@fullcalendar/adaptive": "workspace:*",
    "@fullcalendar/core": "workspace:*",
    "@fullcalendar/daygrid": "workspace:*",
    "@fullcalendar/interaction": "workspace:*",
    "@fullcalendar/list": "workspace:*",
    "@fullcalendar/resource-common": "workspace:*",
    "@fullcalendar/resource-daygrid": "workspace:*",
    "@fullcalendar/resource-timegrid": "workspace:*",
    "@fullcalendar/resource-timeline": "workspace:*",
    "@fullcalendar/scrollgrid": "workspace:*",
    "@fullcalendar/timegrid": "workspace:*",
    "@fullcalendar/timeline": "workspace:*",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@fullcalendar/workspace-scripts": "workspace:*"
  },
  "scripts": {
    "build": "workspace-scripts pkg:build",
    "clean": "workspace-scripts pkg:clean",
    "lint": "eslint ."
  },
  "tsConfig": {
    "extends":  "@fullcalendar/workspace-scripts/config/tsconfig.browser.json",
    "compilerOptions": {
      "outDir": "./dist/.tsout"
    }
  },
  "buildConfig": {
    "exports": {
      ".": { "iife": "FullCalendar" }
    },
    "iifeExternals": false
  },
  "publishConfig": {
    "directory": "./dist",
    "linkDirectory": true
  }
}
