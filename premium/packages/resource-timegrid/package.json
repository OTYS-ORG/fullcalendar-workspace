{
  "name": "@fullcalendar/resource-timegrid",
  "version": "6.0.0-beta.1",
  "title": "FullCalendar Resource Time Grid Plugin",
  "description": "Displays events on a vertical resource view with time slots",
  "docs": "https://fullcalendar.io/docs/vertical-resource-view",
  "type": "module",
  "dependencies": {
    "@fullcalendar/core": "workspace:*",
    "@fullcalendar/premium-common": "workspace:*",
    "@fullcalendar/resource-common": "workspace:*",
    "@fullcalendar/resource-daygrid": "workspace:*",
    "@fullcalendar/timegrid": "workspace:*",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@fullcalendar/workspace-scripts": "workspace:*"
  },
  "scripts": {
    "build": "workspace-scripts pkg:build",
    "clean": "workspace-scripts pkg:clean",
    "lint": "eslint ."
  },
  "buildConfig": {
    "iifeExternals": {
      "@fullcalendar/core": "FullCalendar",
      "@fullcalendar/core/preact": "FullCalendar",
      "@fullcalendar/premium-common": "FullCalendarPremiumCommon",
      "@fullcalendar/resource-common": "FullCalendarResourceCommon",
      "@fullcalendar/resource-daygrid": "FullCalendarResourceDayGrid",
      "@fullcalendar/timegrid": "FullCalendarTimeGrid",
      "tslib": false
    },
    "exports": {
      ".": { "iife": "FullCalendarResourceTimeGrid" }
    }
  },
  "publishConfig": {
    "directory": "./dist",
    "linkDirectory": true
  }
}
