{
  "name": "@fullcalendar/premium-common",
  "version": "6.0.0-beta.1",
  "title": "FullCalendar Premium Common",
  "description": "Utilities for all of FullCalendar Scheduler's plugins",
  "type": "module",
  "dependencies": {
    "@fullcalendar/core": "workspace:*",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@fullcalendar/workspace-scripts": "workspace:*"
  },
  "scripts": {
    "build": "workspace-scripts pkg:build",
    "clean": "workspace-scripts pkg:clean"
  },
  "buildConfig": {
    "iifeExternals": {
      "@fullcalendar/core": "FullCalendar",
      "@fullcalendar/core/preact": "FullCalendar",
      "tslib": false
    },
    "exports": {
      ".": { "iife": "FullCalendarPremiumCommon" }
    }
  },
  "publishConfig": {
    "directory": "./dist",
    "linkDirectory": true
  }
}
