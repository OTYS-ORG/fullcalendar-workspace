{
  "name": "@fullcalendar/adaptive",
  "version": "6.0.0-beta.1",
  "title": "FullCalendar Adaptive Plugin",
  "description": "Optimizes FullCalendar for print (and soon other types of devices)",
  "docs": "https://fullcalendar.io/docs/scheduler",
  "homepage": "https://fullcalendar.io/scheduler",
  "bugs": "https://fullcalendar.io/reporting-bugs",
  "repository": {
    "type": "git",
    "url": "https://github.com/fullcalendar/fullcalendar-scheduler.git",
    "homepage": "https://github.com/fullcalendar/fullcalendar-scheduler"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "author": {
    "name": "Adam Shaw",
    "email": "arshaw@arshaw.com",
    "url": "http://arshaw.com/"
  },
  "type": "module",
  "dependencies": {
    "@fullcalendar/core": "workspace:*",
    "@fullcalendar/premium-common": "workspace:*",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@fullcalendar/workspace-scripts": "workspace:*"
  },
  "scripts": {
    "build": "workspace-scripts pkg:build",
    "clean": "workspace-scripts pkg:clean"
  },
  "buildConfig": {
    "exports": {
      ".": {
        "iife": {
          "name": "FullCalendarAdaptive",
          "globals": {
            "@fullcalendar/core": "FullCalendar",
            "@fullcalendar/core/preact": "FullCalendar"
          }
        }
      }
    }
  },
  "publishConfig": {
    "directory": "./dist",
    "linkDirectory": true
  }
}
