{
  "name": "@fullcalendar/resource-timeline",
  "version": "6.0.0-beta.1",
  "title": "FullCalendar Resource Timeline Plugin",
  "description": "Display events and resources on a horizontal time axis",
  "docs": "https://fullcalendar.io/docs/timeline-view",
  "type": "module",
  "dependencies": {
    "@fullcalendar/core": "workspace:*",
    "@fullcalendar/premium-common": "workspace:*",
    "@fullcalendar/resource-common": "workspace:*",
    "@fullcalendar/scrollgrid": "workspace:*",
    "@fullcalendar/timeline": "workspace:*",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@fullcalendar/workspace-scripts": "workspace:*"
  },
  "scripts": {
    "build": "workspace-scripts pkg:build",
    "clean": "workspace-scripts pkg:clean"
  },
  "buildConfig": {
    "exports": {
      ".": {
        "iife": {
          "name": "FullCalendarResourceTimelinePlugin",
          "globals": {
            "@fullcalendar/core": "FullCalendar",
            "@fullcalendar/core/preact": "FullCalendar"
          }
        }
      }
    }
  },
  "publishConfig": {
    "directory": "./dist",
    "linkDirectory": true
  }
}
