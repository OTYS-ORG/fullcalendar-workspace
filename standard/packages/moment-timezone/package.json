{
  "name": "@fullcalendar/moment-timezone",
  "version": "7.0.0-beta.0",
  "title": "FullCalendar Moment Timezone Plugin",
  "description": "Enhanced named time zone functionality with Moment Timezone",
  "keywords": [
    "moment",
    "moment-timezone",
    "timezone"
  ],
  "homepage": "https://fullcalendar.io/docs/moment-timezone-plugin",
  "peerDependencies": {
    "@fullcalendar/core": "~7.0.0-beta.0",
    "moment-timezone": "^0.5.40"
  },
  "devDependencies": {
    "@fullcalendar/core": "~7.0.0-beta.0",
    "@fullcalendar-scripts/standard": "0.0.1-beta.0",
    "moment-timezone": "^0.5.40"
  },
  "scripts": {
    "build": "standard-scripts pkg:build",
    "clean": "standard-scripts pkg:clean",
    "lint": "eslint ."
  },
  "type": "module",
  "tsConfig": {
    "extends": "@fullcalendar-scripts/standard/config/tsconfig.browser.json",
    "compilerOptions": {
      "rootDir": "./src",
      "outDir": "./dist/.tsout"
    },
    "include": [
      "./src/**/*"
    ]
  },
  "buildConfig": {
    "exports": {
      ".": {
        "iife": true
      }
    },
    "iifeGlobals": {
      ".": "FullCalendar.MomentTimezone",
      "moment-timezone": "moment"
    }
  },
  "publishConfig": {
    "directory": "./dist",
    "linkDirectory": true
  }
}
