{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["src/**/*", "package.json"],
      "outputs": ["dist/**/*", "!**/*.map", "!dist/.tsc/**/*"]
    },
    "dev": {
      "dependsOn": [],
      "inputs": ["src/**/*", "package.json"],
      "outputs": ["dist/**/*", "!dist/.tsc/**/*"]
    },
    "clean": {
      "dependsOn": []
    }
  }
}
