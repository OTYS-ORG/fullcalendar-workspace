{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["package.json", "dist/.tsc/**/*"],
      "outputs": ["dist/**/*.{mjs,cjs,js,json}", "!dist/.tsc/**/*"]
    },
    "clean": {
      "cache": false,
      "dependsOn": []
    }
  }
}
