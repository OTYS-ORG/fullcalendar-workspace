{
  "private": true,
  "name": "@fullcalendar/workspace",
  "version": "0.0.0",
  "type": "module",
  "devDependencies": {
    "@fullcalendar/workspace-scripts": "workspace:*",
    "turbo": "^1.4.6"
  },
  "scripts": {
    "pnpm:devPreinstall": "workspace-scripts root:preflight",
    "preflight": "workspace-scripts root:preflight",
    "build": "workspace-scripts root:build",
    "clean": "turbo run clean --no-cache"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=7.8.0"
  },
  "pnpm": {
    "packageExtensions": {
      "@nuxt/components": {
        "peerDependenciesMeta": {
          "consola": {
            "optional": true
          }
        }
      }
    },
    "peerDependencyRules": {
      "allowAny": [
        "postcss"
      ]
    },
    "patchedDependencies": {
      "@pnpm/make-dedicated-lockfile@0.3.20": "scripts/patches/@pnpm__make-dedicated-lockfile@0.3.20.patch",
      "concurrently@7.3.0": "scripts/patches/concurrently@7.3.0.patch"
    }
  }
}
