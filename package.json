{
  "private": true,
  "name": "@fullcalendar/workspace",
  "version": "0.0.0",
  "type": "module",
  "devDependencies": {
    "@fullcalendar/workspace-scripts": "workspace:*",
    "turbo": "^1.4.6"
  },
  "scripts": {
    "pnpm:devPreinstall": "workspace-scripts preflight",
    "preflight": "workspace-scripts preflight",
    "dir": "pnpm -r --filter './standard/**' --filter './premium/**' exec mkdir -p dist",
    "build": "turbo run build --filter './standard/**' --filter './premium/**'",
    "tsc:dev": "tsc -b --watch --pretty --preserveWatchOutput",
    "clean": "turbo run clean --no-cache",
    "turbo:clean": "rm -rf node_modules/.cache/turbo && find . -type d -name '.turbo' -exec rm -rf {} \\;"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=7.8.0"
  },
  "pnpm": {
    "packageExtensions": {
      "@nuxt/components": {
        "peerDependenciesMeta": {
          "consola": {
            "optional": true
          }
        }
      }
    },
    "peerDependencyRules": {
      "allowAny": [
        "postcss"
      ]
    },
    "patchedDependencies": {
      "@pnpm/make-dedicated-lockfile@0.3.20": "scripts/patches/@pnpm__make-dedicated-lockfile@0.3.20.patch",
      "concurrently@7.3.0": "scripts/patches/concurrently@7.3.0.patch"
    }
  }
}
