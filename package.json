{
  "private": true,
  "name": "@fullcalendar/workspace",
  "version": "0.0.0",
  "type": "module",
  "devDependencies": {
    "@fullcalendar/workspace-scripts": "workspace:*"
  },
  "scripts": {
    "pnpm:devPreinstall": "pnpm run preflight",
    "preflight": "pnpm run -r --parallel preflight"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=7.8.0"
  },
  "pnpm": {
    "packageExtensions": {
      "@nuxt/components": {
        "peerDependenciesMeta": {
          "consola": {
            "optional": true
          }
        }
      }
    },
    "peerDependencyRules": {
      "allowAny": ["postcss"]
    },
    "patchedDependencies": {
      "@pnpm/make-dedicated-lockfile@0.3.20": "scripts/patches/@pnpm__make-dedicated-lockfile@0.3.20.patch",
      "concurrently@7.3.0": "scripts/patches/concurrently@7.3.0.patch"
    }
  }
}
